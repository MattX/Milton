{
  "name": "appengine-typescript",
  "description": "An example TypeScript app running on Google App Engine.",
  "version": "0.0.2",
  "private": true,
  "license": "Apache Version 2.0",
  "author": "Bar Cart Coffee Club",
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "scribe.js",
  "scripts": {
    "test": "mocha test/*.test.js --exit",
    "lint": "tslint -p .",
    "start": "node ./index.js",
    "gcp-build": "tsc -p .",
    "deploy-appengine": "gcloud app deploy",
    "deploy-function": "gcloud functions deploy scribe --runtime nodejs10 --allow-unauthenticated --trigger-http --memory 512MB"
  },
  "dependencies": {
    "dompurify": "^2.0.8",
    "express": "^4.16.3",
    "jsdom": "^16.2.0",
    "readability": "github:mozilla/readability",
    "request-promise-native": "latest",
    "typescript": "^3.8.1"
  },
  "devDependencies": {
    "mocha": "^7.0.0",
    "wait-port": "^0.2.7",
    "@types/express": "^4.16.0",
    "@types/dompurify": "^2.0.1",
    "chai": "^4.2.0",
    "tslint": "^6.0.0"
  }
}
